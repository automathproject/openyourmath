{
  "name": "svelte-virtual",
  "version": "0.6.3",
  "type": "module",
  "author": "Pedro AmÃ©rico",
  "license": "MIT",
  "repository": "github:ghostebony/svelte-virtual",
  "description": "Virtual List and Grid components for Svelte",
  "keywords": [
    "svelte",
    "virtual",
    "grid",
    "list",
    "scroll"
  ],
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "pnpm": ">=7"
  },
  "peerDependencies": {
    "svelte": "^3.54.0 || ^4.0.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.1.1",
    "@sveltejs/kit": "^2.5.1",
    "@sveltejs/package": "^2.2.7",
    "@sveltejs/vite-plugin-svelte": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.1",
    "publint": "^0.2.7",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.4",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.1.4"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js"
    },
    "./Grid.svelte": {
      "types": "./dist/Grid.svelte.d.ts",
      "svelte": "./dist/Grid.svelte"
    },
    "./List.svelte": {
      "types": "./dist/List.svelte.d.ts",
      "svelte": "./dist/List.svelte"
    }
  },
  "files": [
    "dist"
  ],
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    ">4.0": {
      "Grid.svelte": [
        "./dist/Grid.svelte.d.ts"
      ],
      "List.svelte": [
        "./dist/List.svelte.d.ts"
      ]
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build && pnpm package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin=prettier-plugin-svelte --check . && eslint .",
    "format": "prettier --plugin=prettier-plugin-svelte --write .",
    "build:package:local": "svelte-kit sync && svelte-package -o dist/dist/"
  }
}