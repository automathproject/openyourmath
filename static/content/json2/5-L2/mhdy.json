      "type": "question",
      "value": {
        "latex": "$\\sum \\frac{n!\\,a_n}{n^n}z^n$.",
        "html": "<p><span class=\"math inline\">\\(\\sum \\frac{n!\\,a_n}{n^n}z^n\\)</span>.</p>\n"
      }
    },
    {
      "id": "54212ca5-f5db-450d-baac-5ecc0b9c4993",
