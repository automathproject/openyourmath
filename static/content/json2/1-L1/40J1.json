      "value": {
        "latex": "$\\dim\\mathrm{Ker}(f\\circ g) \\le \\dim\\mathrm{Ker} f + \\dim\\mathrm{Ker} g$.",
        "html": "<p><span class=\"math inline\">\\(\\dim\\mathrm{Ker}(f\\circ g) \\le \\dim\\mathrm{Ker} f + \\dim\\mathrm{Ker} g\\)</span>.</p>\n"
