    },
    {
      "id": "fc62b4cc-1a1d-4152-9580-c34338d4cb39",
      "type": "reponse",
      "value": {
        "latex": "$\\frac n{2^{n-1}}$.",
        "html": "<p><span class=\"math inline\">\\(\\frac n{2^{n-1}}\\)</span>.</p>\n"
      }
