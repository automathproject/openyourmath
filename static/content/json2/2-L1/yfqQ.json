      "type": "question",
      "value": {
        "latex": "$A \\subset B \\Rightarrow \\inf A \\leqslant \\inf B$,",
        "html": "<p><span class=\"math inline\">\\(A \\subset B \\Rightarrow \\inf A \\leqslant \\inf B\\)</span>,</p>\n"
