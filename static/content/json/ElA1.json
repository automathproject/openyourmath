  "titre": "Ouverts et fermés dans $\\R^2$",
  "theme": "topologie",
  "auteur": "",
  "date": "",
  "organisation": "AMSCC",
  "contenu": [
    {
      "type": "question",
      "value": "<p>Déterminer si les ensembles suivants sont ouverts ou fermés dans <span class=\"math inline\">\\(\\R^2\\)</span>.</p>\n"
    },
    {
      "type": "question",
      "value": "<p><span class=\"math inline\">\\(A=\\{(x,y)\\in \\R^2 \\mid 0&lt;\\vert x-1\\vert &lt;1 \\}\\)</span> ;</p>\n"
    },
    {
      "type": "reponse",
      "value": "<p>ouvert</p>\n"
    },
    {
      "type": "question",
      "value": "<p><span class=\"math inline\">\\(B=\\{(x,y)\\in \\R^2 \\mid 0\\leq x\\leq y\\}\\)</span> ;</p>\n"
    },
    {
      "type": "reponse",
      "value": "<p>fermé</p>\n"
    },
    {
      "type": "question",
      "value": "<p><span class=\"math inline\">\\(C=\\{(x,y)\\in \\R^2 \\mid \\vert x\\vert &lt;1,\\; \\vert y\\vert \\leq\n    1 \\}\\)</span></p>\n"
    },
    {
      "type": "reponse",
      "value": "<p>ni ouvert, ni fermé</p>\n"
    }
  ]
}